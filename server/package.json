{
  "name": "server",
  "scripts": {
    "start": "dotenv -- nodemon -e ts,graphql -x ts-node src/index.ts",
    "dev": "npm-run-all --parallel start playground",
    "debug": "dotenv -- nodemon -e ts,graphql -x ts-node --inspect src/index.ts",
    "playground": "graphql playground",
    "build": "rimraf dist && tsc",
    "lint": "tslint -c tslint.json --fix 'src/**/*.ts'",
    "gen-types": "gql-gen --require ts-node/register --template graphql-codegen-typescript-template --schema ./src/importSchema.ts --out ./src/typings/"
  },
  "dependencies": {
    "bcryptjs": "2.4.3",
    "graphql-yoga": "1.14.12",
    "jsonwebtoken": "^8.3.0",
    "prisma-binding": "2.1.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.1",
    "@types/graphql": "^0.13.3",
    "@types/jsonwebtoken": "^7.2.8",
    "dotenv-cli": "1.4.0",
    "graphql-cli": "2.16.4",
    "graphql-code-generator": "^0.10.3",
    "graphql-codegen-typescript-template": "^0.10.3",
    "graphql-import": "^0.6.0",
    "nodemon": "1.18.2",
    "npm-run-all": "4.1.3",
    "prisma": "1.11.1",
    "rimraf": "2.6.2",
    "ts-node": "6.2.0",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.9.2",
    "typescript": "2.9.2"
  }
}
